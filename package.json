{
  "name": "testapp",
  "version": "1.0.0",
  "description": "React Native Application for Amazon Vega OS/Kepler",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build bundle kepler-build node_modules package-lock.json generated coverage",
    "build": "npm-run-all compile lint:fix",
    "compile": "tsc -p tsconfig.json",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "build:release": "react-native build-kepler --build-type Release",
    "build:debug": "react-native build-kepler --build-type Debug",
    "build:app": "npm-run-all build:release build:debug"
  },
  "dependencies": {
    "@amazon-devices/headless-task-manager": "^1.1.0",
    "@amazon-devices/kepler-ui-components": "^2.0.0",
    "@amazon-devices/react-native-kepler": "^2.0.0",
    "@amazon-devices/react-native-gesture-handler": "~2.0.0",
    "@amazon-devices/react-native-reanimated": "~2.0.0",
    "@amazon-devices/react-native-safe-area-context": "~2.0.0",
    "@amazon-devices/react-native-screens": "~2.0.0",
    "@amazon-devices/react-native-svg": "~2.0.0",
    "@amazon-devices/react-native-w3cmedia": "~2.1.0",
    "@amazon-devices/react-navigation__core": "~7.0.0",
    "@amazon-devices/react-navigation__native": "~7.0.0",
    "@amazon-devices/react-navigation__stack": "~7.0.0",
    "@amazon-devices/react-native-async-storage__async-storage": "~2.0.2+1.23.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@reduxjs/toolkit": "~2.2.4",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-redux": "~9.1.2"
  },
  "devDependencies": {
    "@amazon-devices/kepler-cli-platform": "^0",
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "11.3.2",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.2.6",
    "@types/react-native": "^0.72.2",
    "babel-jest": "^29.2.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "npm-run-all": "~4.1.5",
    "prettier": "^2.4.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "author": "",
  "license": "MIT",
  "keywords": [
    "react-native",
    "vega-os",
    "kepler",
    "firetv",
    "amazon",
    "tv"
  ],
  "kepler": {
    "projectType": "application",
    "appName": "TestApp",
    "targets": [
      "tv"
    ],
    "os": [
      "vega"
    ],
    "api": 0.1
  }
}
